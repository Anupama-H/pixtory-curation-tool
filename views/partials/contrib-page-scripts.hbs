{{#ifEqual mode "development"}}
    <script type="text/javascript" src="/portal/javascripts/lib/jquery-1.12.4.min.js"></script>
    <script type="text/javascript" src="/portal/javascripts/lib/jquery-handlebars-1.1.5.min.js"></script>
    <script type="text/javascript" src="/portal/javascripts/lib/js.cookie.js"></script>
    <script type="text/javascript">
        /* handlebars templating configs */
        $.handlebars({
            templatePath: "/portal/templates",
            templateExtension: "hbs"
        });
        var App = {
            loginRedirect: "/portal/contributor/pushedPixtories",
            logoutRedirect: "/portal/contributor/login"
        };
    </script>
    <script type="text/javascript" src="/portal/javascripts/utils.js"></script>
    <script type="text/javascript" src="/portal/javascripts/events.js"></script>
    {{#ifEqual page "LOGIN"}}
    <script type="text/javascript" src="/portal/javascripts/login.js"></script>
    {{else}}
    <script type="text/javascript" src="/portal/javascripts/check-login.js"></script>
    {{/ifEqual}}
    <!-- Login initialization script -->
    <script type="text/javascript" src="/portal/javascripts/login-init.js"></script>
    <!-- Include page specific scripts -->
    {{#ifEqual page "CREATE"}}
    <script type="text/javascript" src="/portal/javascripts/create-pixtory.js"></script>
    {{/ifEqual}}
    {{#ifEqual page "PUSHED"}}
    <script type="text/javascript" src="/portal/javascripts/contrib-pushed-pixtory.js"></script>
    {{/ifEqual}}
    {{#ifEqual page "SUBMITTED"}}
    <script type="text/javascript" src="/portal/javascripts/contrib-all-pixtory.js"></script>
    {{/ifEqual}}
    {{#ifEqual page "NOTIF"}}
    <script type="text/javascript" src="/portal/javascripts/contrib-notifications.js"></script>
    {{/ifEqual}}
    <script type="text/javascript" src="/portal/javascripts/app.js"></script>
{{else}}
    <script type="text/javascript" src="/portal/javascripts/dist/libs.min.js?v={{assetVersion}}"></script>
    <script type="text/javascript">
        /* handlebars templating configs */
