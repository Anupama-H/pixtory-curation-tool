        };
    </script>
    <script type="text/javascript" src="/portal/javascripts/dist/utils.min.js?v={{assetVersion}}"></script>
    <script type="text/javascript" src="/portal/javascripts/dist/events.min.js?v={{assetVersion}}"></script>
    {{#ifEqual page "LOGIN"}}
    <script type="text/javascript" src="/portal/javascripts/dist/login.min.js?v={{assetVersion}}"></script>
    {{else}}
    <script type="text/javascript" src="/portal/javascripts/dist/check-login.min.js?v={{assetVersion}}"></script>
    {{/ifEqual}}
    <!-- Login initialization script -->
    <script type="text/javascript" src="/portal/javascripts/dist/login-init.min.js?v={{assetVersion}}"></script>
    <!-- Include page specific scripts -->
    {{#ifEqual page "CREATE"}}
    <script type="text/javascript" src="/portal/javascripts/dist/create-pixtory.min.js?v={{assetVersion}}"></script>
